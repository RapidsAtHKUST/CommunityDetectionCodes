	$(TROFF) -t -e -mandoc -Tascii $< | $(COL) -bx > $@

.7.txt:
	$(TROFF) -t -e -mandoc -Tascii $< | $(COL) -bx > $@

.azm.html:
	$(ZOEM) -d html -i $< -o $@
	$(ZOEM) -d html -i $< -o $@
	- $(TIDY) -e $@

